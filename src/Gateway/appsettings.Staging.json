{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Seq", "Serilog.Sinks.Console" ],
    "WriteTo": [
      { 
        "Name": "Seq", 
        "Args": {
          "serverUrl": "https://capp-kairos-seq.salmonpebble-5905d3a4.eastus2.azurecontainerapps.io:5341",
          "apiKey": "Serilog:WriteTo:0:Args:apiKey"
        }
      },
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "({ThreadId}) [{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj} - CorrelationId: {CorrelationId} - Hostname: {MachineName} - Logger: {SourceContext} {NewLine}{Exception}"
        }
      }
    ]
  },
  "Health": { "Seq": { "Url": "https://capp-kairos-seq.salmonpebble-5905d3a4.eastus2.azurecontainerapps.io:443/health" } },
  "EventBus": {
    "HostAddress": "EventBus:HostAddress",
    "Events": { },
    "Commands": { }
  }
}
